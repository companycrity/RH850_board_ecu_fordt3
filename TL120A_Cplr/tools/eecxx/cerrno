// cerrno standard header
#ifndef _CERRNO_
#define _CERRNO_

#if defined(__ghs) || defined(__SC3__)

#include <errno.h>

#else /* !__ghs */

#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <errno.h>
 #define _STD_USING

 #ifndef errno
  #define errno	errno
 #endif /* errno */

#else /* _STD_USING */
 #include <errno.h>

 #ifdef _GLOBAL_USING
_STD_BEGIN
  #ifndef errno
   #define errno	errno
using _CSTD errno;
  #endif /* errno */

_STD_END
 #endif /* _GLOBAL_USING */

#endif /* _CERRNO_ */

#endif /* !__ghs */
#endif /* _STD_USING */

