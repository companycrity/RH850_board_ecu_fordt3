// cassert standard header
#if defined(__ghs) || defined(__SC3__)
#if !defined assert
extern "C" void assert(int);
#if defined(__NAMESPACES) && !defined(__EMBEDDED_CXX_HEADERS)
namespace std { using ::assert; }
#endif // __NAMESPACES
#endif

#include <assert.h>
#else /* !__ghs */

#include <yvals.h>
#include <assert.h>

#endif /* !__ghs */
