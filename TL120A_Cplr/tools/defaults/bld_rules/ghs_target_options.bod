	    MemoryDescriptionFile {
	    #### For all targets ##
		    name="--memory_description_file"
		    delimiter="Equal"
		    merge="Replace"
		    optionType="String"
		    pass={"Link"}
		    disabled=false
		    guiLabel="Memory Description File"
		    guiCategory="Target"
		    commonLevel=101
		    hidden=true
	    }
	    RegisterDescriptionFile {
	    #### For all targets ##
		    name="--register_definition_file"
