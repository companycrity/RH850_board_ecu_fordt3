RTEServConnectionEditor_read_connection = "                                   \
                                                                              \
/*    STRINGLIB_msg = \"Reading connection\"; STRINGLIB_show_msg(); */        \
                                                                              \
    allArgs = $TCGUILIB_conargs;                                              \
                                                                              \
/* Set some defaults */                                                       \
                                                                              \
    $tfOtherOptions = \"\";                                                   \
                                                                              \
    $pdMulticoreSupport = \"Disabled\";                                       \
    cpunamecnt = 0;                                                           \
                                                                              \
/* Set argv to first token */                                                 \
                                                                              \
    STRINGLIB_init();                                                         \
    STRINGLIB_string = allArgs;                                               \
    argv = STRINGLIB_get_next_token();                                        \
                                                                              \
/* Parse args */                                                              \
                                                                              \
    while (strlen (argv) > 0) {                                               \
                                                                              \
        if (argv == \"-stp\") {                                               \
            argv = STRINGLIB_get_next_token();                                \
                                                                              \
            if (argv == \"-usb\") {                                           \
                $pdSTP = \"USB\";                                             \
                                                                              \
            } else {                                                          \
                $pdSTP = \"ethernet\";                                        \
                $txtStpAddress.dim = 0;                                       \
                $txfStpAddress.dim = 0;                                       \
                $txfStpAddress = argv;                                        \
            }                                                                 \
                                                                              \
        } else if (argv == \"-multicore\") {                                  \
            $pdMulticoreSupport = \"Enabled\";                                \
            $pdSTP = \"no STP\";                                              \
            $pdSTP.dim = 1;                                                   \
            $txtSTP.dim = 1;                                                  \
        } else if (argv == \"-cpu\") {                                        \
/* just eat the CPU name since I don't think that matters */                  \
            argv = STRINGLIB_get_next_token();                                \
            cpunamecnt = cpunamecnt+1;                                        \
        } else if( strlen( $tfOtherOptions ) == 0 ) {                         \
            $tfOtherOptions = argv;                                           \
        } else {                                                              \
            $tfOtherOptions = $tfOtherOptions | \" \" | argv;                 \
        }                                                                     \
                                                                              \
/* Advance argv to next token */                                              \
                                                                              \
        argv = STRINGLIB_get_next_token();                                    \
    }                                                                         \
                                                                              \
    if ($pdMulticoreSupport == \"Enabled\") {                                 \
            if ((cpunamecnt > 0) && (cpunamecnt < 3)) {                       \
                $pdNumCores = text(cpunamecnt);                               \
           } else if (cpunamecnt > 2) {                                       \
            $pdNumCores = \"2\";                                              \
            } else {                                                          \
            $pdNumCores = \"1\";                                              \
            }                                                                 \
    } else {                                                                  \
        $pdNumCores = \"1\";                                                  \
    }                                                                         \
                                                                              \
                                                                              \
                                                                              \
"
RTEServConnectionEditor_save_connection = "                                   \
                                                                              \
/*    STRINGLIB_msg = \"Saving connection\"; STRINGLIB_show_msg(); */         \
                                                                              \
    argsAccum = \"\";                                                         \
                                                                              \
/* Add Other Options last */                                                  \
                                                                              \
    if ($pdSTP == \"ethernet\" && strlen($txfStpAddress) > 0) {               \
        argsAccum = argsAccum | \" -stp \" | $txfStpAddress;                  \
    } else if ($pdSTP == \"USB\") {                                           \
        argsAccum = argsAccum | \" -stp -usb\";                               \
    }                                                                         \
                                                                              \
    if ($pdMulticoreSupport == \"Enabled\") {                                 \
        argsAccum = argsAccum | \" -multicore\";                              \
    }                                                                         \
                                                                              \
    cpucnt = number($pdNumCores);                                             \
    cpunum = 1;                                                               \
    while ((cpunum <= cpucnt) && ($pdMulticoreSupport == \"Enabled\")) {      \
            argsAccum = argsAccum | \" -cpu\";                                \
        cpuname = \"PE\" | text(cpunum);                                      \
        argsAccum = argsAccum | \" \" | cpuname;                              \
        cpunum = cpunum + 1;                                                  \
    }                                                                         \
                                                                              \
    if( strlen( $tfOtherOptions ) > 0 ) {                                     \
      argsAccum = argsAccum | \" \" | $tfOtherOptions;                        \
    }                                                                         \
                                                                              \
    $TCGUILIB_conargs = argsAccum;                                            \
