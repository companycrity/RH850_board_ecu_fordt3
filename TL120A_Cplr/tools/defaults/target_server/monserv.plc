MonServConnectionEditor_onSetup = "                                           \
                                                                              \
    clitext( \"setpswindowname \\\"Green Hills Monitor (monserv) Connection Editor\\\"\" ); \
                                                                              \
    hostName    = clitext( \"__gethostname\" );                               \
                                                                              \
    if( index( hostName, \"windows\" ) != -1 ) {                              \
        activate( 1 );                                                        \
    } else {                                                                  \
        activate( 2 );                                                        \
    }                                                                         \
                                                                              \
    SelectSerialPort_onSetup();                                               \
    SelectBaud_onSetup();                                                     \
    OtherOptions_onSetup();                                                   \
                                                                              \
                                                                              \
/* set the callbacks and properties the TCGUILIB wants */                     \
    $TCGUILIB_editor_dlg_name      = \"MonServConnectionEditor\";             \
    $TCGUILIB_editor_saveargs_fn   = \"MonServConnectionEditor_save_connection\"; \
    $TCGUILIB_editor_readargs_fn   = \"MonServConnectionEditor_read_connection\"; \
    $TCGUILIB_editor_evalargs_fn   = \"MonServConnectionEditor_eval_connection\"; \
    $TCGUILIB_statuswin_dlg_name   = \"MonServConnectionStatus\";             \
                                                                              \
    TCGUILIB_editor_onSetup();                                                \
