MonServConnectionEditor_read_connection = "                                   \
                                                                              \
/*    STRINGLIB_msg = \"Reading connection\"; STRINGLIB_show_msg(); */        \
                                                                              \
    allArgs = $TCGUILIB_conargs;                                              \
                                                                              \
/* Set some defaults */                                                       \
                                                                              \
    port_specified = 0;                                                       \
    baud_specified = 0;                                                       \
                                                                              \
    $tfOtherOptions = \"\";                                                   \
    $btnText.set = 1;                                                         \
    $btnData.set = 1;                                                         \
    $btnBSS.set  = 0;                                                         \
    $btnDebugOutput.set       = 0;                                            \
    $btnstDebugOutputDest.dim = 1;                                            \
    $btnstHaltMethod.set(1)   = 1;                                            \
                                                                              \
                                                                              \
/* Set argv to first token */                                                 \
                                                                              \
    STRINGLIB_init();                                                         \
    STRINGLIB_string = allArgs;                                               \
    argv = STRINGLIB_get_next_token();                                        \
                                                                              \
/* Parse args */                                                              \
                                                                              \
    while( strlen( argv ) > 0 ) {                                             \
                                                                              \
/* STRINGLIB_msg = \"argv: \" | argv; STRINGLIB_show_msg(); */                \
                                                                              \
        if( argv == \"-debug_mon\" ) {                                        \
            $btnDebugOutput.set          = 1;                                 \
            $btnstDebugOutputDest.dim    = 0;                                 \
            $btnstDebugOutputDest.set(1) = 1;                                 \
            argv = STRINGLIB_get_next_token();                                \
                                                                              \
        } else if( argv == \"-logfile\" ) {                                   \
            $btnstDebugOutputDest.set(2) = 1;                                 \
            argv = STRINGLIB_get_next_token();                                \
                                                                              \
        } else if( argv == \"-noload\" ) {                                    \
            $btnText.set = 0;                                                 \
            $btnData.set = 0;                                                 \
            $btnBSS.set  = 0;                                                 \
                                                                              \
        } else if( argv == \"-loadall\" ) {                                   \
            $btnText.set = 1;                                                 \
            $btnData.set = 1;                                                 \
            $btnBSS.set  = 1;                                                 \
                                                                              \
        } else if( argv == \"-notext\" ) {                                    \
            $btnText.set = 0;                                                 \
                                                                              \
        } else if( argv == \"-text\" ) {                                      \
            $btnText.set = 1;                                                 \
                                                                              \
        } else if( argv == \"-nodata\" ) {                                    \
            $btnData.set = 0;                                                 \
                                                                              \
        } else if( argv == \"-data\" ) {                                      \
            $btnData.set = 1;                                                 \
                                                                              \
        } else if( argv == \"-nobss\" ) {                                     \
            $btnBSS.set = 0;                                                  \
                                                                              \
        } else if( argv == \"-bss\" ) {                                       \
            $btnBSS.set = 1;                                                  \
                                                                              \
        } else if( argv == \"-zbreak\" ) {                                    \
            $btnstHaltMethod.set(2)   = 1;                                    \
                                                                              \
        } else if( argv == \"-dbreak\" ) {                                    \
            $btnstHaltMethod.set(3)   = 1;                                    \
                                                                              \
        } else if( argv == \"-baud\" ) {                                      \
            $pdBaud = STRINGLIB_get_next_token();                             \
            baud_specified = 1;                                               \
                                                                              \
        } else if( argv == \"-sp\" ) {                                        \
            if( index( hostName, \"windows\" ) != -1 ) {                      \
                $pdSerialPortWindows = STRINGLIB_get_next_token();            \
            } else {                                                          \
                $pdSerialPortUnix    = STRINGLIB_get_next_token();            \
            }                                                                 \
            port_specified = 1;                                               \
                                                                              \
        } else {                                                              \
            if (index (argv, \"-\") == 0) {                                   \
/* this starts with a dash, so I'll bet its an option */                      \
                if( strlen( $tfOtherOptions ) == 0 ) {                        \
                    $tfOtherOptions = argv;                                   \
                } else {                                                      \
                    $tfOtherOptions = $tfOtherOptions | \" \" | argv;         \
                }                                                             \
                                                                              \
                                                                              \
            } else if (port_specified == 0) {                                 \
/* this must be the serial port */                                            \
                if( index( hostName, \"windows\" ) != -1 ) {                  \
                    $pdSerialPortWindows = argv;                              \
                } else {                                                      \
                    $pdSerialPortUnix    = argv;                              \
                }                                                             \
                port_specified = 1;                                           \
                                                                              \
                                                                              \
            } else if (baud_specified == 0) {                                 \
/* this must be a baud rate */                                                \
                $pdBaud = argv;                                               \
                baud_specified = 1;                                           \
                                                                              \
                                                                              \
            } else {                                                          \
/* okay, it really is another option */                                       \
                if( strlen( $tfOtherOptions ) == 0 ) {                        \
                    $tfOtherOptions = argv;                                   \
                } else {                                                      \
                    $tfOtherOptions = $tfOtherOptions | \" \" | argv;         \
                }                                                             \
            }                                                                 \
        }                                                                     \
                                                                              \
/* Advance argv to next token */                                              \
                                                                              \
        argv = STRINGLIB_get_next_token();                                    \
    }                                                                         \
                                                                              \
                                                                              \
                                                                              \
"
MonServConnectionEditor_save_connection = "                                   \
                                                                              \
/*    STRINGLIB_msg = \"Saving connection\"; STRINGLIB_show_msg(); */         \
    argsAccum = \"\";                                                         \
                                                                              \
                                                                              \
    if ($btnDebugOutput.set == 1) {                                           \
        if ($btnstDebugOutputDest.set(1) == 1) {                              \
            argsAccum = argsAccum | \" -debug_mon 15\";                       \
        } else if ($btnstDebugOutputDest.set(2) == 1) {                       \
            argsAccum = argsAccum | \" -debug_mon 15 -logfile monserv.log\";  \
        }                                                                     \
    }                                                                         \
                                                                              \
                                                                              \
    if ($btnText.set == 0 &&                                                  \
        $btnData.set == 0 &&                                                  \
        $btnBSS.set  == 0) {                                                  \
        argsAccum = argsAccum | \" -noload\";                                 \
    } else if ($btnText.set == 1 &&                                           \
               $btnData.set == 1 &&                                           \
               $btnBSS.set  == 1) {                                           \
        argsAccum = argsAccum | \" -loadall\";                                \
    } else {                                                                  \
        if ($btnText.set == 0) {                                              \
            argsAccum = argsAccum | \" -notext\";                             \
        }                                                                     \
        if ($btnData.set == 0) {                                              \
            argsAccum = argsAccum | \" -nodata\";                             \
        }                                                                     \
        if ($btnBSS.set == 1) {                                               \
            argsAccum = argsAccum | \" -bss\";                                \
        }                                                                     \
    }                                                                         \
                                                                              \
                                                                              \
    if ($pdSerialPortWindows != \"<default>\") {                              \
        argsAccum = argsAccum | \" -sp \" | $pdSerialPortWindows;             \
    } else if ($pdSerialPortUnix != \"<default>\") {                          \
        argsAccum = argsAccum | \" -sp \" | $pdSerialPortUnix;                \
    }                                                                         \
                                                                              \
    STRINGLIB_init();                                                         \
    STRINGLIB_string = $pdBaud;                                               \
    if ($pdBaud != \"<default>\" && STRINGLIB_count_tokens() > 0) {           \
        argsAccum = argsAccum | \" -baud \" | $pdBaud;                        \
    }                                                                         \
                                                                              \
    if ($btnstHaltMethod.set(2) == 1) {                                       \
        argsAccum = argsAccum | \" -zbreak\";                                 \
    } else if ($btnstHaltMethod.set(3) == 1) {                                \
        argsAccum = argsAccum | \" -dbreak\";                                 \
    }                                                                         \
                                                                              \
                                                                              \
    if (strlen($tfOtherOptions) > 0) {                                        \
        argsAccum = argsAccum | \" \" | $tfOtherOptions;                      \
    }                                                                         \
                                                                              \
                                                                              \
    $TCGUILIB_conargs = argsAccum;                                            \
                                                                              \
                                                                              \
                                                                              \
