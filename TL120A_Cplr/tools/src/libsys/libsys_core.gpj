#!gbuild
[Library]
	-ansi
	--incorrect_pragma_warnings
	--unknown_pragma_warnings
	--prototype_warnings
	--diag_error 236
	--no_coverage_analysis
	-check=nomemory
	{!isdefined(SUPPRESS_FNONE)} -fnone
	-pnone
	-Onoinline
ind_crt1.c
	{optional} -argcheck=none
	{optional} -globalreg=99
	-gtws
ind_call.gpj		[Select One]
ind_dots.gpj		[Select One]
ind_exit.c
	-gtws
ind_core.c
