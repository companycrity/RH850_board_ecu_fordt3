GetEnumListItem = "                                                           \
                                                                              \
/* Requests the value for an enum list */                                     \
    result = clitext (\"IsEnumListOptionSet optionID:\" | text(optionid) |    \
                      \" choice:\\\"\" | choice | \"\\\"\");                  \
                                                                              \
    if (strlen (antonym) > 0) {                                               \
        aresult = clitext (\"IsEnumListOptionSet optionID:\" | text(optionid) | \
                           \" choice:\\\"\" | antonym | \"\\\"\");            \
    } else {                                                                  \
        aresult = \"- { isset: 0 isinherited: 0 } \";                         \
    }                                                                         \
                                                                              \
/* is it set? */                                                              \
    setval = 0;                                                               \
    message = result;                                                         \
    numbername = \"isset\";                                                   \
    GetNumber ();                                                             \
    if (value > 0) {                                                          \
        setval = 1;                                                           \
    }                                                                         \
                                                                              \
/* is it inherited? */                                                        \
    ival = 0;                                                                 \
    message = result;                                                         \
    numbername = \"isinherited\";                                             \
    GetNumber ();                                                             \
    if (value > 0) {                                                          \
        ival = 1;                                                             \
    }                                                                         \
                                                                              \
/* What about the antonym? */                                                 \
    message = aresult;                                                        \
    numbername = \"isset\";                                                   \
    GetNumber ();                                                             \
    if (value > 0) {                                                          \
        setval = -1;                                                          \
    }                                                                         \
                                                                              \
    message = aresult;                                                        \
    numbername = \"isinherited\";                                             \
    GetNumber ();                                                             \
    if (value > 0) {                                                          \
        ival = -1;                                                            \
    }                                                                         \
                                                                              \
                                                                              \
                                                                              \
"
GetInheritedOptionText = "                                                    \
                                                                              \
    mergedvalue =                                                             \
        clitext (\"GetInheritedOptionText optionID:\" | text(optionid));      \
                                                                              \
    GetInheritedOptionText_lf = index (mergedvalue, \"\\\"\");                \
    GetInheritedOptionText_rt = rindex (mergedvalue, \"\\\"\");               \
    mergedvalue =                                                             \
        substr (mergedvalue, GetInheritedOptionText_lf+1,                     \
                GetInheritedOptionText_rt - GetInheritedOptionText_lf - 1);   \
                                                                              \
                                                                              \
