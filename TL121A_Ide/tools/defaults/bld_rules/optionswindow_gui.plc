EditOption = "                                                                \
                                                                              \
    EditOption_result =                                                       \
        clitext (\"EditOptionByID optionID:\" | text(optionid) |              \
                 \" allowGoto:\" | text(allowgoto));                          \
                                                                              \
    EditOption_lf = index (EditOption_result, \"changed:\");                  \
    EditOption_ln = strlen (EditOption_result);                               \
    trimtext = substr (EditOption_result, EditOption_lf+8,                    \
                       EditOption_ln - EditOption_lf - 8);                    \
                                                                              \
/* trim the front of the changed number */                                    \
    TrimString ();                                                            \
                                                                              \
/* strip off the back of the number */                                        \
    EditOption_lf = index (trimtext, \" \");                                  \
    optionchanged = number (substr (trimtext, 0, EditOption_lf));             \
                                                                              \
/* now let's get the \"didgoto\" part */                                      \
    EditOption_lf = index (EditOption_result, \"didgoto:\");                  \
    EditOption_rt = rindex (EditOption_result, \" \");                        \
    trimtext = substr (EditOption_result, EditOption_lf+8,                    \
                       EditOption_rt - EditOption_lf - 8);                    \
    TrimString ();                                                            \
    usergoto = number (trimtext);                                             \
                                                                              \
                                                                              \
                                                                              \
"
EditOptionByName = "                                                          \
    GetOptionID ();                                                           \
    EditOption ();                                                            \
                                                                              \
                                                                              \
                                                                              \
"
FillPulldownWithOptionChoices = "                                             \
    FPDWOC_result =                                                           \
        clitext (\"GetOptionChoices optionID:\" | text(optionid));            \
                                                                              \
/* rebuild the response string so that it looks like what */                  \
/* PopulatePulldown is going to want.  Add in the \"default\" choice */       \
/* since we want that to always be available */                               \
                                                                              \
    FPDWOC_lf = index (FPDWOC_result, \"data: (\");                           \
    FPDWOC_rt = strlen (FPDWOC_result);                                       \
    FPDWOC_datastr = \"\\\"<default>\\\" \" | substr (FPDWOC_result, FPDWOC_lf+7, \
                                                FPDWOC_rt-FPDWOC_lf-7);       \
                                                                              \
    FPDWOC_lf = index (FPDWOC_result, \"count: \");                           \
    FPDWOC_countstr = substr (FPDWOC_result, FPDWOC_lf+7,                     \
                              FPDWOC_rt - FPDWOC_lf - 7);                     \
    FPDWOC_rt = index (FPDWOC_countstr, \" \");                               \
    trimtext = substr (FPDWOC_countstr, 0, FPDWOC_rt);                        \
    TrimString ();                                                            \
    FPDWOC_count = number (trimtext) + 1;                                     \
                                                                              \
    FPDWOC_result = substr (FPDWOC_result, 0, FPDWOC_lf) | \"count: \" |      \
        text(FPDWOC_count) | \" data: (\" | FPDWOC_datastr;                   \
                                                                              \
    FPDWOC_lf = index (FPDWOC_result, \"- {\");                               \
    FPDWOC_rt = strlen (FPDWOC_result);                                       \
    FPDWOC_choices = substr (FPDWOC_result, FPDWOC_lf+3,                      \
                             FPDWOC_rt-FPDWOC_lf-3);                          \
    cli (\"PopulatePulldown { pdname:\\\"\" | pulldownname | \"\\\" \" |      \
         FPDWOC_choices);                                                     \
                                                                              \
                                                                              \
/* Now decide what is supposed to be selected. */                             \
    GetOptionText ();                                                         \
    if (thisvalue == \"\") {                                                  \
        thisvalue = \"<default>\";                                            \
    }                                                                         \
                                                                              \
    cli (\"SetPulldown { pdname:\\\"\" | pulldownname |                       \
         \"\\\" choice: \\\"\" | thisvalue | \"\\\"\");                       \
                                                                              \
                                                                              \
                                                                              \
