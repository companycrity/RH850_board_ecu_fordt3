"
RTServ2ConnectionEditor_save_connection = "                                   \
                                                                              \
/*    STRINGLIB_msg = \"Saving connection\"; STRINGLIB_show_msg(); */         \
                                                                              \
    if( $btnEthernet.set == 1 ) {                                             \
        argsAccum = serverName;                                               \
                                                                              \
        use_crc = 0;                                                          \
                                                                              \
        if( index($tfTargetName, \"-crc \") == 0) {                           \
            argsAccum = argsAccum | \" -crc\";                                \
/* Temporarily remove -crc from the hostname.  It will be replaced at */      \
/* the end of this block. */                                                  \
            $tfTargetName = substr($tfTargetName, 5, strlen($tfTargetName)-5); \
            use_crc = 1;                                                      \
        }                                                                     \
                                                                              \
/* only accept one token */                                                   \
        STRINGLIB_init();                                                     \
        STRINGLIB_string = $tfTargetName;                                     \
        STRINGLIB_stop_chars = \" ,\";                                        \
                                                                              \
        $tfTargetName = STRINGLIB_get_next_token ();                          \
                                                                              \
/* Look for port number, if any */                                            \
        colon_index = index( $tfTargetName, \":\" );                          \
        if( colon_index != -1 ) {                                             \
/* Get the base hostname */                                                   \
            udpname = substr( $tfTargetName, 0, colon_index );                \
/* Get the port number */                                                     \
            portnum = substr( $tfTargetName, colon_index+1,                   \
                    strlen( $tfTargetName ) - (colon_index+1) );              \
            argsAccum = argsAccum | \" -udp \" | udpname | \":\" | portnum;   \
        } else {                                                              \
/* no target is equivalent to localhost */                                    \
/* if using crc, localhost must be specified */                               \
            if( use_crc == 1 || $tfTargetName != \"localhost\" ) {            \
                argsAccum = argsAccum | \" \" | $tfTargetName;                \
            }                                                                 \
        }                                                                     \
                                                                              \
        if( use_crc == 1) {                                                   \
            $tfTargetName = \"-crc \" | $tfTargetName;                        \
        }                                                                     \
                                                                              \
    } else {                                                                  \
                                                                              \
        argsAccum = serverName;                                               \
    }                                                                         \
                                                                              \
    if( $btnDebugOutput.set == 1 ) {                                          \
        if( $btnstDebugOutputDest.set(2) == 1 ) {                             \
            argsAccum = argsAccum | \" -log=\\\"\" | $tfDebugOutputFile | \"\\\"\"; \
        }                                                                     \
        else {                                                                \
            argsAccum = argsAccum | \" -watch\";                              \
        }                                                                     \
    }                                                                         \
                                                                              \
    if( $btnSerial.set == 1 ) {                                               \
                                                                              \
        argsAccum = argsAccum | \" -serial\";                                 \
                                                                              \
        if( $pdSerialPortWindows != \"<default>\" ) {                         \
            argsAccum = argsAccum | \" \" | $pdSerialPortWindows;             \
        } else if( $pdSerialPortUnix != \"<default>\" ) {                     \
            argsAccum = argsAccum | \" \" | $pdSerialPortUnix;                \
        } else if (index( hostName, \"windows\" ) != -1) {                    \
/* Default windows serial port */                                             \
            argsAccum = argsAccum | \" COM1\";                                \
        } else {                                                              \
/* Default unix serial port */                                                \
            argsAccum = argsAccum | \" /dev/tty\";                            \
        }                                                                     \
        if( $pdBaud != \"<default>\" ) {                                      \
            argsAccum = argsAccum | \" \" | $pdBaud;                          \
        }                                                                     \
    }                                                                         \
                                                                              \
/* Add Other Options last */                                                  \
                                                                              \
    if( strlen( $tfOtherOptions ) > 0 ) {                                     \
        argsAccum = argsAccum | \" \" | $tfOtherOptions;                      \
    }                                                                         \
                                                                              \
    $TCGUILIB_conargs = argsAccum;                                            \
                                                                              \
                                                                              \
