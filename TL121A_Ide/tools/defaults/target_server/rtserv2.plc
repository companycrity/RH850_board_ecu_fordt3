    $pdBaud = \"<default>\";                                                  \
    $tfDebugOutputFile = \"rtserv2.log\";                                     \
                                                                              \
    RTServ2ConnectionEditor_handle_btnEthernet();                             \
                                                                              \
/* set the callbacks and properties the TCGUILIB wants */                     \
    $TCGUILIB_editor_dlg_name      = \"RTServ2ConnectionEditor\";             \
    $TCGUILIB_editor_saveargs_fn   = \"RTServ2ConnectionEditor_save_connection\"; \
    $TCGUILIB_editor_readargs_fn   = \"RTServ2ConnectionEditor_read_connection\"; \
    $TCGUILIB_editor_evalargs_fn   = \"RTServ2ConnectionEditor_eval_connection\"; \
    $TCGUILIB_statuswin_dlg_name   = \"RTServ2ConnectionStatus\";             \
                                                                              \
/*hide(TCGUILIB_txtConnectFor); */                                            \
                                                                              \
    TCGUILIB_editor_onSetup();                                                \
